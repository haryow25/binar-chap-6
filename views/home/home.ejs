    <!--about gallery -->
    <div class="about-gallery" id="gallery">
      <div class="container">
        <div class="row">
          <div class="col-6 left">
            <h3>What’s so special?</h3>
            <h1>THE GAMES</h1>
          </div>
          <div class="col-6 right">
            <div id="carouselExampleIndicators" class="carousel slide" data-bs-ride="carousel">
              <div class="carousel-indicators">
                <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
                <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="1" aria-label="Slide 2"></button>
                <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="2" aria-label="Slide 3"></button>
              </div>
              <div class="carousel-inner">
                <div class="carousel-item active">
                  <img src="./img/rockpaperstrategy-1600.jpg" class="d-block w-100" alt="..." />
                </div>
                <div class="carousel-item">
                  <img src="./img/rockpaperstrategy-1600.jpg" class="d-block w-100" alt="..." />
                </div>
                <div class="carousel-item">
                  <img src="./img/rockpaperstrategy-1600.jpg" class="d-block w-100" alt="..." />
                </div>
              </div>
              <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Previous</span>
              </button>
              <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Next</span>
              </button>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- end of about gallery -->

    <!-- features -->
    <div class="features">
      <div class="container">
        <div class="row">
          <div class="col-12 left">
            <h3>What’s so special?</h3>
            <h1>features</h1>
          </div>
          <div class="col-12 right">
            <!-- <img src="/asset/Ellipse 1.svg" alt="">
            <img src="/asset/progress bar.png" alt="">
            <img src="/asset/Ellipse.svg" alt="">
            <img src="/asset/Ellipse.svg" alt=""> -->
            <ul>
              <li>TRADTIONAL GAMES</li>
              <p>If you miss your childhood, we provide many traditional games here</p>
              <li>GAME SUIT</li>
              <li>TBD</li>
            </ul>
          </div>
        </div>
      </div>
    </div>
    <!-- end of features -->

    <!-- requirement -->
    <div class="requirement">
      <div class="container">
        <h3>Can My Computer Run this game?</h3>
        <div class="row top">
          <div class="col">
            <h1>system requirements</h1>
          </div>
        </div>

        <div class="row bottom">
          <div class="col-6">
            <p class="text-warning">OS:</p>
            <p>Windows 7 64-bit only (No OSX support at this time)</p>
          </div>
          <div class="col-6">
            <p class="text-warning">PROCESSOR:</p>
            <p>Intel Core 2 Duo @ 2.4 GHZ or AMD Athlon X2 @ 2.8 GHZ</p>
          </div>
          <div class="col-6">
            <p class="text-warning">MEMORY:</p>
            <p>4 GB RAM</p>
          </div>
          <div class="col-6">
            <p class="text-warning">STORAGE:</p>
            <p>8 GB available space</p>
          </div>
          <div class="col-12">
            <p class="text-warning">GRAPHICS:</p>
            <p>NVIDIA GeForce GTX 660 2GB or AMD Radeon HD 7850 2GB DirectX11 (Shader Model 5)</p>
          </div>
        </div>
      </div>
    </div>
    <!-- end of requirement -->

    <!-- quote -->
    <div class="quote">
      <div class="container">
        <div class="row">
          <div class="col-6 left">
            <h1>TOP SCORES</h1>
            <p>This top score from various games provided on this platform</p>
            <button type="button" class="btn btn-warning">SEE MORE</button>
          </div>
          <div class="col-6 right">
            <div class="card top">
              <div class="img-container">
                <img src="./img/image 8.jpg" class="card-img-top" alt="..." />
                <div class="card-name">
                  <p class="name">Evan Lahti</p>
                  <p class="work">PC Gamer</p>
                </div>
                <img src="./img/twitter.svg" class="icon" alt="" />
              </div>

              <div class="card-body">
                <p class="card-text">“One of my gaming highlights of the year.”</p>
                <p class="date">June 18, 2021</p>
              </div>
            </div>
            <div class="card middle">
              <div class="img-container">
                <img src="./img/image.jpg" class="card-img-top" alt="..." />
                <div class="card-name">
                  <p class="name">Jada Griffin</p>
                  <p class="work">Nerdreactor</p>
                </div>
                <img src="./img/twitter.svg" class="icon" alt="" />
              </div>
              <div class="card-body">
                <p class="card-text">“The next big thing in the world of streaming and survival games.”</p>
                <p class="date">July 10, 2021</p>
              </div>
            </div>
            <div class="card bottom">
              <div class="img-container">
                <img src="./img/image-1.jpg" class="card-img-top" alt="..." />
                <div class="card-name">
                  <p class="name">Aaron Williams</p>
                  <p class="work">Uproxx</p>
                </div>
                <img src="./img/twitter.svg" class="icon" alt="" />
              </div>
              <div class="card-body">
                <p class="card-text">“Snoop Dogg Playing The Wildly Entertaining ‘SOS’ Is Ridiculous.”</p>
                <p class="date">December 24, 2018</p>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- end of quote -->

    <!-- newsletter -->
    <div class="newsletter">
      <div class="container">
        <div class="row">
          <div class="col-5">
            <img src="./img/image .png" alt="" />
          </div>
          <div class="col-7">
            <h3>Want to stay in touch?</h3>
            <h1>newsletter subscribe</h1>
            <p>In order to start receiving our news, all you have to do is enter your email address. Everything else will be taken care of by us. We will send you emails containing information about game. We don’t spam.</p>
            <div class="email">
              <input type="email" class="form-control" id="exampleFormControlInput1" placeholder="youremail@binar.co,id" />
              <button type="button" class="btn btn-warning">Subscribe now</button>
            </div>
          </div>
        </div>
      </div>
